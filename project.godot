config_version=5

[application]

config/name="MonsterMatch"
run/main_scene="res://src/TitleScreen/TitleScreen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globals="*res://src/Singletons/Globals.gd"
ValidationServer="*res://src/Singletons/ValidationServer.gd"
BackgroundMusicHandler="*res://src/Audio/BackgroundMusicHandler.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1600
window/size/window_height_override=900
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="MonsterMatch"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

